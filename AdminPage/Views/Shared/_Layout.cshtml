    <div class="container-fluid">
    <div class="row">
            <div class="col-md-2 col-sm-12 p-0  min-vh-100" style="background-color:#27365E">
                <div class="w-100" style="height:140px">
                    <a asp-action="Index" asp-controller="Home">

                    <img class="w-100 h-100" src="https://i.pinimg.com/736x/0e/3c/00/0e3c004c4c5692d06d359b6b2f10eaed.jpg" alt="Alternate Text" />
